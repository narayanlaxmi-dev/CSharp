@model IList<Student>

<h2>This is Student page index page</h2>

<h2>Student List</h2>
<table class="table">
    <thead>
        <tr class="table-primary">
            <th scope="col">Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td scope="row">your Id is @student.Id</td>
                <td>Yoo @student.Name </td>
            </tr>
        }
    </tbody>
</table>


